    "mysql": "^2.17.1",
    "mysql2": "^1.7.0",
    "neal-react": "^0.2.5",
    "owl.carousel": "^2.3.4",
    "pure-react-carousel": "^1.23.0",
    "react-animated-text": "^0.1.4",
    "react-awesome-slider": "^2.0.0",
    "react-bootstrap-slider": "^2.2.2",
    "react-js-banner": "^0.2.4",
